  "bugs": {
    "url": "https://github.com/IanaStepanova/accessibility-training/issues"
  },
  "homepage": "https://github.com/IanaStepanova/accessibility-training#readme",
  "devDependencies": {
    "bulma": "^0.9.3",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "browser-sync": "^2.27.5",
    "npx": "^10.2.2"
  }
}
